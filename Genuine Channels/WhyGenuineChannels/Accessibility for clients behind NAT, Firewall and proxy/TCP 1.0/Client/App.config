<configuration>
	<system.runtime.remoting>
		<application>
      		<lifetime
      		leaseTime="5M"
      		sponsorshipTimeout="5M"
      		renewOnCallTime="5M"
      		leaseManagerPollTime="1M"
      		/>
			<channels>
				<channel ref="tcp" port="0"/>
			</channels>
		</application>
	</system.runtime.remoting>
	<appSettings>
		<add 
			key="RemoteHostUri"
			value="tcp://127.0.0.1:8737"
		/>
	</appSettings>
</configuration>
