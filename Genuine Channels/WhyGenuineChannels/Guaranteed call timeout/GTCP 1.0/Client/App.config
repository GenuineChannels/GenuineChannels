<configuration>
	<system.runtime.remoting>
		<application>
      		<lifetime
      			leaseTime="10M"
      			sponsorshipTimeout="10M"
      			renewOnCallTime="10M"
      			leaseManagerPollTime="1M"
      		/>

			<channels>
				<channel type="Belikov.GenuineChannels.GenuineTcp.GenuineTcpChannel, GenuineChannels"
         		/>
			</channels>
		</application>
	</system.runtime.remoting>
	<appSettings>
		<add 
			key="RemoteHostUri"
			value="gtcp://127.0.0.1:8737"
		/>
	</appSettings>
</configuration>
